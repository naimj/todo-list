onMounted(async () => {
    try {
        // Load world map data
        const worldMapData = await fetch('https://raw.githubusercontent.com/holtzy/D3-graph-gallery/master/DATA/world.geojson');
        const worldGeoJSON = await worldMapData.json();
        registerMap('world', worldGeoJSON);
        mapLoaded.value = true;
    } catch (error) {
        console.error('Failed to load world map data:', error);
    }
});
